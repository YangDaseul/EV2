<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/btn_bottom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="bottom dialog"
                android:textColor="@color/x_000000"
                android:textSize="11sp"/>


            <TextView
                android:id="@+id/btn_genesis_membership"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="genesis-membership"
                android:textColor="@color/x_000000"
                android:textSize="11sp"/>


            <TextView
                android:id="@+id/btn_calender"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="calender"
                android:textColor="@color/x_000000"
                android:textSize="11sp"/>


            <TextView
                android:id="@+id/btn_graph_bar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="bar chart"
                android:textColor="@color/x_000000"
                android:textSize="11sp"/>

            <TextView
                android:id="@+id/btn_graph_stackedbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="stacked bar chart"
                android:textColor="@color/x_000000"
                android:textSize="11sp"/>

            <TextView
                android:id="@+id/btn_graph_pie"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="pie chart"
                android:textColor="@color/x_000000"
                android:textSize="11sp"/>


            <TextView
                android:id="@+id/btn_ga_purchase"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="구매상담신청"
                android:textColor="@color/x_000000"
                android:textSize="11sp"/>
            <TextView
                android:id="@+id/btn_ga_drive"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="시승신청"
                android:textColor="@color/x_000000"
                android:textSize="11sp"/>
            <TextView
                android:id="@+id/btn_ga_similar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="유사차량"
                android:textColor="@color/x_000000"
                android:textSize="11sp"/>

            <TextView
                android:id="@+id/btn_bto_main"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="BTO MAIN"
                android:textColor="@color/x_000000"
                android:textSize="11sp"/>


        </LinearLayout>

        </ScrollView>
    </FrameLayout>
</layout>