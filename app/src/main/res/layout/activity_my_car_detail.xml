<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="activity"
            type="com.genesis.apps.ui.main.home.MyCarDetailActivity" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/l_whole"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/x_f4f4f4">

        <include
            android:id="@+id/l_title"
            layout="@layout/layout_titlebar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:value="@{@string/gm_carlst_04_21}" />

        <ScrollView
            android:id="@+id/l_scroll"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/l_title">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="30dp"
                android:paddingBottom="96dp">


                <include
                    android:id="@+id/l_car"
                    layout="@layout/item_my_car"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="1.128:1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <TextView
                    android:id="@+id/tv_title_mobility_care_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:ellipsize="end"
                    android:fontFamily="@font/light_genesissansheadglobal"
                    android:gravity="left"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="8sp"
                    android:maxLines="1"
                    android:text="@string/gm_carlst_04_5"
                    android:textColor="@color/x_000000"
                    android:textSize="22sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/l_car" />

                <TextView
                    android:id="@+id/tv_title_mobility_care_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/regular_genesissansheadglobal"
                    android:gravity="left"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="14sp"
                    android:maxLines="1"
                    android:text="@string/gm_carlst_04_6"
                    android:textColor="@color/x_000000"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="@+id/tv_title_mobility_care_1"
                    app:layout_constraintEnd_toEndOf="@+id/tv_title_mobility_care_1"
                    app:layout_constraintTop_toBottomOf="@+id/tv_title_mobility_care_1" />


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/l_part_engine"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/bg_ffffff_stroke_e2e2e2"
                    android:paddingLeft="15dp"
                    android:paddingTop="20dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="20dp"
                    app:layout_constraintStart_toStartOf="@+id/tv_title_mobility_care_1"
                    app:layout_constraintEnd_toEndOf="@+id/tv_title_mobility_care_1"
                    app:layout_constraintTop_toBottomOf="@+id/tv_title_mobility_care_2">


                    <TextView
                        android:id="@+id/tv_title_part_engine"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:gravity="left"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:maxLines="1"
                        android:text="@string/gm_carlst_04_7"
                        android:textColor="@color/x_000000"
                        android:textSize="16sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <TextView
                        android:id="@+id/tv_title_part_engine_oil"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:fontFamily="@font/light_genesissansheadglobal"
                        android:gravity="left"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:maxLines="1"
                        android:text="@string/gm_carlst_04_8"
                        android:textColor="@color/x_000000"
                        android:textSize="16sp"
                        app:layout_constraintHorizontal_bias=".25"
                        app:layout_constraintLeft_toRightOf="@+id/tv_title_part_engine"
                        app:layout_constraintRight_toLeftOf="@+id/tv_part_engine_oil_cnt"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/tv_part_engine_oil_cnt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:textColor="@color/x_cd9a81"
                        android:textSize="16sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>


                    <TextView
                        android:id="@+id/tv_title_part_oil_filter"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:ellipsize="end"
                        android:fontFamily="@font/light_genesissansheadglobal"
                        android:gravity="left"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:maxLines="1"
                        android:text="@string/gm_carlst_04_9"
                        android:textColor="@color/x_000000"
                        android:textSize="16sp"
                        app:layout_constraintHorizontal_bias="0"
                        app:layout_constraintStart_toStartOf="@+id/tv_title_part_engine_oil"
                        app:layout_constraintRight_toLeftOf="@+id/tv_part_engine_oil_cnt"
                        app:layout_constraintTop_toBottomOf="@+id/tv_title_part_engine_oil" />


                    <TextView
                        android:id="@+id/tv_part_oil_filter_cnt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:textColor="@color/x_cd9a81"
                        android:textSize="16sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/tv_title_part_oil_filter"/>


                    <TextView
                        android:id="@+id/tv_title_part_air_cleaner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:ellipsize="end"
                        android:fontFamily="@font/light_genesissansheadglobal"
                        android:gravity="left"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:maxLines="1"
                        android:text="@string/gm_carlst_04_10"
                        android:textColor="@color/x_000000"
                        android:textSize="16sp"
                        app:layout_constraintHorizontal_bias="0"
                        app:layout_constraintStart_toStartOf="@+id/tv_title_part_engine_oil"
                        app:layout_constraintRight_toLeftOf="@+id/tv_part_engine_oil_cnt"
                        app:layout_constraintTop_toBottomOf="@+id/tv_title_part_oil_filter" />

                    <TextView
                        android:id="@+id/tv_part_air_cleaner_cnt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:textColor="@color/x_cd9a81"
                        android:textSize="16sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/tv_title_part_air_cleaner"/>
                </androidx.constraintlayout.widget.ConstraintLayout>



                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/l_part_air_conditioner_filter"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:background="@drawable/bg_ffffff_stroke_e2e2e2"
                    android:paddingLeft="15dp"
                    android:paddingTop="20dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="20dp"
                    app:layout_constraintTop_toBottomOf="@+id/l_part_engine"
                    app:layout_constraintLeft_toLeftOf="@+id/l_part_engine"
                    app:layout_constraintRight_toRightOf="@+id/l_part_engine">


                    <TextView
                        android:id="@+id/tv_title_part_air_conditioner_filter"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:gravity="left"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:maxLines="1"
                        android:text="@string/gm_carlst_04_11"
                        android:textColor="@color/x_000000"
                        android:textSize="16sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <TextView
                        android:id="@+id/tv_part_air_conditioner_filter_cnt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:textColor="@color/x_cd9a81"
                        android:textSize="16sp"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>

                </androidx.constraintlayout.widget.ConstraintLayout>



                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/l_part_break_pad"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:background="@drawable/bg_ffffff_stroke_e2e2e2"
                    android:paddingLeft="15dp"
                    android:paddingTop="20dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="20dp"
                    app:layout_constraintTop_toBottomOf="@+id/l_part_air_conditioner_filter"
                    app:layout_constraintLeft_toLeftOf="@+id/l_part_engine"
                    app:layout_constraintRight_toRightOf="@+id/l_part_engine">


                    <TextView
                        android:id="@+id/tv_title_part_break_pad"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:gravity="left"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:maxLines="1"
                        android:text="@string/gm_carlst_04_12"
                        android:textColor="@color/x_000000"
                        android:textSize="16sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <TextView
                        android:id="@+id/tv_part_break_pad_cnt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:textColor="@color/x_cd9a81"
                        android:textSize="16sp"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/l_part_wiper"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:background="@drawable/bg_ffffff_stroke_e2e2e2"
                    android:paddingLeft="15dp"
                    android:paddingTop="20dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="20dp"
                    app:layout_constraintTop_toBottomOf="@+id/l_part_break_pad"
                    app:layout_constraintLeft_toLeftOf="@+id/l_part_engine"
                    app:layout_constraintRight_toRightOf="@+id/l_part_engine">


                    <TextView
                        android:id="@+id/tv_title_part_wiper"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:gravity="left"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:maxLines="1"
                        android:text="@string/gm_carlst_04_13"
                        android:textColor="@color/x_000000"
                        android:textSize="16sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <TextView
                        android:id="@+id/tv_part_wiper_cnt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:textColor="@color/x_cd9a81"
                        android:textSize="16sp"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>

                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/l_part_break"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:background="@drawable/bg_ffffff_stroke_e2e2e2"
                    android:paddingLeft="15dp"
                    android:paddingTop="20dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="20dp"
                    app:layout_constraintTop_toBottomOf="@+id/l_part_wiper"
                    app:layout_constraintLeft_toLeftOf="@+id/l_part_engine"
                    app:layout_constraintRight_toRightOf="@+id/l_part_engine">


                    <TextView
                        android:id="@+id/tv_title_part_break"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:gravity="left"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:maxLines="1"
                        android:text="@string/gm_carlst_04_14"
                        android:textColor="@color/x_000000"
                        android:textSize="16sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <TextView
                        android:id="@+id/tv_part_break_cnt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:textColor="@color/x_cd9a81"
                        android:textSize="16sp"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>

                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/l_part_home"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:background="@drawable/bg_ffffff_stroke_e2e2e2"
                    android:paddingLeft="15dp"
                    android:paddingTop="20dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="20dp"
                    app:layout_constraintTop_toBottomOf="@+id/l_part_break"
                    app:layout_constraintLeft_toLeftOf="@+id/l_part_engine"
                    app:layout_constraintRight_toRightOf="@+id/l_part_engine">


                    <TextView
                        android:id="@+id/tv_title_part_home"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:gravity="left"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:maxLines="1"
                        android:text="@string/gm_carlst_04_15"
                        android:textColor="@color/x_000000"
                        android:textSize="16sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <TextView
                        android:id="@+id/tv_part_home_cnt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/regular_genesissansheadglobal"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="2sp"
                        android:textColor="@color/x_cd9a81"
                        android:textSize="16sp"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>

                </androidx.constraintlayout.widget.ConstraintLayout>



            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/l_privilege"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:padding="20dp"
            android:background="@drawable/bg_141414_top_round_20"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent">


            <TextView
                android:id="@+id/tv_title_privilege"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/light_genesissansheadglobal"
                android:letterSpacing="-0.01"
                android:lineSpacingExtra="0sp"
                android:textColor="@color/x_ffffff"
                android:textSize="16sp"
                android:text="@string/gm_carlst_04_17"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <TextView
                android:id="@+id/btn_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/light_genesissansheadglobal"
                android:letterSpacing="-0.01"
                android:lineSpacingExtra="0sp"
                android:paddingLeft="14dp"
                android:paddingRight="14dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColor="@color/x_dbdbdb"
                android:textSize="14sp"
                android:text="@string/gm_carlst_04_19"
                android:background="@drawable/ripple_bg_6f6f6f_stroke_dbdbdb"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>


            <TextView
                android:id="@+id/btn_benefit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="6dp"
                android:fontFamily="@font/light_genesissansheadglobal"
                android:letterSpacing="-0.01"
                android:lineSpacingExtra="0sp"
                android:paddingLeft="14dp"
                android:paddingRight="14dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColor="@color/x_141414"
                android:textSize="14sp"
                android:text="@string/gm_carlst_04_18"
                android:background="@drawable/ripple_bg_ffffff"
                app:layout_constraintEnd_toStartOf="@+id/btn_status"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>


            <TextView
                android:id="@+id/btn_apply"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/light_genesissansheadglobal"
                android:letterSpacing="-0.01"
                android:lineSpacingExtra="0sp"
                android:paddingLeft="14dp"
                android:paddingRight="14dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColor="@color/x_141414"
                android:textSize="14sp"
                android:text="@string/gm_carlst_04_20"
                android:background="@drawable/ripple_bg_ffffff"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:visibility="visible"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>