<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="activity"
            type="com.genesis.apps.ui.myg.MyGMembershipInfoActivity" />
    </data>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/l_whole"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/x_ffffff">

        <include
            android:id="@+id/l_title"
            layout="@layout/layout_titlebar_depth3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/l_title">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingBottom="30dp">


                <TextView
                    android:id="@+id/tv_membership_info"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="20dp"
                    android:fontFamily="@font/light_genesissansheadglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="8sp"
                    android:text="@string/gm_carlst_01_p01_1"
                    android:textColor="@color/x_000000"
                    android:textSize="22sp"
                    app:layout_constrainedWidth="true"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tv_number_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:fontFamily="@font/regular_genesissanstextglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="6sp"
                    android:text="1."
                    android:textColor="@color/x_aaabaf"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@+id/tv_membership_info"
                    app:layout_constraintTop_toBottomOf="@+id/tv_membership_info" />


                <TextView
                    android:id="@+id/tv_contents_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="11dp"
                    android:fontFamily="@font/regular_genesissanstextglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="6sp"
                    android:text="@string/gm_carlst_01_p01_2"
                    android:textColor="@color/x_000000"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toEndOf="@id/tv_number_1"
                    app:layout_constraintTop_toTopOf="@+id/tv_number_1" />


                <View
                    android:id="@+id/v_line_1"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/x_f4f4f4"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toStartOf="@+id/tv_contents_1"
                    app:layout_constraintTop_toBottomOf="@+id/tv_contents_1" />


                <TextView
                    android:id="@+id/tv_number_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/regular_genesissanstextglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="6sp"
                    android:text="2."
                    android:textColor="@color/x_aaabaf"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@+id/tv_membership_info"
                    app:layout_constraintTop_toBottomOf="@+id/v_line_1" />


                <TextView
                    android:id="@+id/tv_contents_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="11dp"
                    android:fontFamily="@font/regular_genesissanstextglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="6sp"
                    android:text="@string/gm_carlst_01_p01_3"
                    android:textColor="@color/x_000000"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toEndOf="@id/tv_number_2"
                    app:layout_constraintTop_toTopOf="@+id/tv_number_2" />


                <View
                    android:id="@+id/v_line_2"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/x_f4f4f4"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toStartOf="@+id/tv_contents_2"
                    app:layout_constraintTop_toBottomOf="@+id/tv_contents_2" />


                <TextView
                    android:id="@+id/tv_number_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/regular_genesissanstextglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="6sp"
                    android:text="3."
                    android:textColor="@color/x_aaabaf"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@+id/tv_membership_info"
                    app:layout_constraintTop_toBottomOf="@+id/v_line_2" />


                <TextView
                    android:id="@+id/tv_contents_3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="11dp"
                    android:fontFamily="@font/regular_genesissanstextglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="6sp"
                    android:text="@string/gm_carlst_01_p01_4"
                    android:textColor="@color/x_000000"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toEndOf="@id/tv_number_3"
                    app:layout_constraintTop_toTopOf="@+id/tv_number_3" />


                <View
                    android:id="@+id/v_line_3"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/x_f4f4f4"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toStartOf="@+id/tv_contents_3"
                    app:layout_constraintTop_toBottomOf="@+id/tv_contents_3" />


                <TextView
                    android:id="@+id/tv_number_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/regular_genesissanstextglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="6sp"
                    android:text="4."
                    android:textColor="@color/x_aaabaf"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@+id/tv_membership_info"
                    app:layout_constraintTop_toBottomOf="@+id/v_line_3" />


                <TextView
                    android:id="@+id/tv_contents_4"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="11dp"
                    android:fontFamily="@font/regular_genesissanstextglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="6sp"
                    android:text="@string/gm_carlst_01_p01_5"
                    android:textColor="@color/x_000000"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toEndOf="@id/tv_number_4"
                    app:layout_constraintTop_toTopOf="@+id/tv_number_4" />


                <View
                    android:id="@+id/v_line_4"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/x_f4f4f4"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toStartOf="@+id/tv_contents_4"
                    app:layout_constraintTop_toBottomOf="@+id/tv_contents_4" />


                <TextView
                    android:id="@+id/tv_number_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/regular_genesissanstextglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="6sp"
                    android:text="5."
                    android:textColor="@color/x_aaabaf"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@+id/tv_membership_info"
                    app:layout_constraintTop_toBottomOf="@+id/v_line_4" />


                <TextView
                    android:id="@+id/tv_contents_5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="11dp"
                    android:fontFamily="@font/regular_genesissanstextglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="6sp"
                    android:text="@string/gm_carlst_01_p01_6"
                    android:textColor="@color/x_000000"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toEndOf="@id/tv_number_5"
                    app:layout_constraintTop_toTopOf="@+id/tv_number_5" />


                <TextView
                    android:id="@+id/tv_sub_contents_5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="21dp"
                    android:fontFamily="@font/regular_genesissansheadglobal"
                    android:letterSpacing="-0.01"
                    android:lineSpacingExtra="10sp"
                    android:text="@string/gm_carlst_01_p01_7"
                    android:textColor="@color/x_000000"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toStartOf="@+id/tv_contents_5"
                    app:layout_constraintTop_toBottomOf="@+id/tv_contents_5" />



                <RelativeLayout
                    android:id="@+id/l_contents_5_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:background="@color/x_f4f4f4"
                    android:padding="14dp"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toStartOf="@+id/tv_contents_5"
                    app:layout_constraintTop_toBottomOf="@+id/tv_sub_contents_5">

                    <TextView
                        android:id="@+id/tv_contents_5_1_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/regular_genesissanstextglobal"
                        android:text="@string/gm_carlst_01_p01_8"
                        android:textColor="@color/x_000000"
                        android:textSize="13sp"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="9sp"/>


                    <TextView
                        android:id="@+id/tv_contents_5_1_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_below="@+id/tv_contents_5_1_1"
                        android:fontFamily="@font/regular_genesissanstextglobal"
                        android:text="@string/gm_carlst_01_p01_9"
                        android:textColor="@color/x_000000"
                        android:textSize="15sp"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="7sp"/>


                    <TextView
                        android:id="@+id/tv_mark_1"
                        style="@style/ViewMarkGrey"
                        android:layout_marginTop="12dp"
                        android:layout_below="@+id/tv_contents_5_1_2"/>


                    <TextView
                        android:id="@+id/tv_contents_5_1_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:layout_below="@+id/tv_contents_5_1_2"
                        android:layout_toRightOf="@+id/tv_mark_1"
                        android:fontFamily="@font/regular_genesissanstextglobal"
                        android:text="@string/gm_carlst_01_p01_10"
                        android:textColor="@color/x_aaabaf"
                        android:textSize="14sp"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="6sp"/>

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/l_contents_5_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:background="@color/x_f4f4f4"
                    android:padding="14dp"
                    app:layout_constraintEnd_toEndOf="@+id/tv_membership_info"
                    app:layout_constraintStart_toStartOf="@+id/tv_contents_5"
                    app:layout_constraintTop_toBottomOf="@+id/l_contents_5_1">

                    <TextView
                        android:id="@+id/tv_contents_5_2_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/regular_genesissanstextglobal"
                        android:text="@string/gm_carlst_01_p01_11"
                        android:textColor="@color/x_000000"
                        android:textSize="13sp"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="9sp"/>


                    <TextView
                        android:id="@+id/tv_contents_5_2_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_below="@+id/tv_contents_5_2_1"
                        android:fontFamily="@font/regular_genesissanstextglobal"
                        android:text="@string/gm_carlst_01_p01_12"
                        android:textColor="@color/x_000000"
                        android:textSize="15sp"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="7sp"/>


                    <TextView
                        android:id="@+id/tv_mark_2"
                        style="@style/ViewMarkGrey"
                        android:layout_marginTop="12dp"
                        android:layout_below="@+id/tv_contents_5_2_2"/>


                    <TextView
                        android:id="@+id/tv_contents_5_2_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:layout_below="@+id/tv_contents_5_2_2"
                        android:layout_toRightOf="@+id/tv_mark_2"
                        android:fontFamily="@font/regular_genesissanstextglobal"
                        android:text="@string/gm_carlst_01_p01_13"
                        android:textColor="@color/x_aaabaf"
                        android:textSize="14sp"
                        android:letterSpacing="-0.01"
                        android:lineSpacingExtra="6sp"/>

                </RelativeLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>