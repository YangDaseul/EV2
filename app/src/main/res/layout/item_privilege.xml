<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/l_whole"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="20dp"
        android:paddingBottom="20dp"
        tools:background="@drawable/bg_ffffff_stroke_e2e2e2">


        <TextView
            android:id="@+id/tv_model"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:fontFamily="@font/regular_genesissansheadglobal"
            android:letterSpacing="-0.01"
            android:lineSpacingExtra="4dp"
            android:maxLines="2"
            android:textColor="@color/x_000000"
            android:textSize="20dp"
            app:layout_constrainedWidth="true"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/barrier_1"
            tools:text="GV 80" />

        <androidx.constraintlayout.widget.Barrier
            android:id="@+id/barrier_1"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            app:barrierDirection="start"
            app:constraint_referenced_ids="btn_apply,btn_benefit" />


        <TextView
            android:id="@+id/btn_apply"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="13dp"
            android:paddingRight="13dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:background="@drawable/ripple_bg_000000"
            android:fontFamily="@font/light_genesissansheadglobal"
            android:letterSpacing="-0.01"
            android:lineSpacingExtra="7dp"
            android:text="@string/mg_prvi01_word_1"
            android:textColor="@color/x_ffffff"
            android:textSize="14dp"
            android:visibility="gone"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_model"
            app:layout_constraintBottom_toBottomOf="@+id/tv_car_rgst_no"
            tools:visibility="visible"/>


        <TextView
            android:id="@+id/btn_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="13dp"
            android:paddingRight="13dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:background="@drawable/ripple_bg_ffffff_stroke_6f6f6f"
            android:fontFamily="@font/light_genesissansheadglobal"
            android:letterSpacing="-0.01"
            android:lineSpacingExtra="7dp"
            android:text="@string/mg_prvi01_word_5"
            android:textColor="@color/x_141414"
            android:textSize="14dp"
            android:visibility="gone"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_model"
            app:layout_constraintBottom_toBottomOf="@+id/tv_car_rgst_no"
            tools:visibility="visible"/>

        <TextView
            android:id="@+id/btn_benefit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="6dp"
            android:paddingLeft="13dp"
            android:paddingRight="13dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:background="@drawable/ripple_bg_ffffff_stroke_6f6f6f"
            android:fontFamily="@font/light_genesissansheadglobal"
            android:letterSpacing="-0.01"
            android:lineSpacingExtra="7dp"
            android:text="@string/mg_prvi01_word_4"
            android:textColor="@color/x_141414"
            android:textSize="14dp"
            android:visibility="gone"
            app:layout_constraintEnd_toStartOf="@+id/btn_status"
            app:layout_constraintTop_toTopOf="@+id/tv_model"
            app:layout_constraintBottom_toBottomOf="@+id/tv_car_rgst_no"
            tools:visibility="visible"/>


        <TextView
            android:id="@+id/tv_car_rgst_no"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:fontFamily="@font/regular_genesissanstextglobal"
            android:letterSpacing="-0.01"
            android:lineSpacingExtra="-2dp"
            android:maxLines="1"
            android:textColor="@color/x_aaabaf"
            android:textSize="18dp"
            android:gravity="left"
            app:layout_constraintTop_toBottomOf="@+id/tv_model"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:text="5인승 22인치 sdks2오토" />



        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:background="@color/x_f9f9f9"
            app:layout_constraintTop_toBottomOf="@+id/tv_car_rgst_no"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">


            <TextView
                android:id="@+id/tv_title_vin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/light_genesissansheadglobal"
                android:letterSpacing="-0.01"
                android:lineSpacingExtra="2dp"
                android:textColor="@color/x_000000"
                android:textSize="16dp"
                android:text="@string/mg_prvi01_word_3"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>


            <TextView
                android:id="@+id/tv_vin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:fontFamily="@font/regular_genesissansheadglobal"
                android:letterSpacing="-0.01"
                android:textColor="@color/x_000000"
                android:textSize="16dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tv_title_vin"
                app:layout_constraintHorizontal_bias="0"
                tools:text="15487ABD45782"/>


        </androidx.constraintlayout.widget.ConstraintLayout>



    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>