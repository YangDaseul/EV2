<resources>

    <!--텍스트 골드-->
    <style name="ViewBackgroundTransStrokeToupe">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:textColor">@color/x_996449</item>
        <item name="android:textSize">12dp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6dp</item>
        <item name="android:gravity">center</item>
    </style>

    <!--내역이 없습니다.-->
    <style name="ViewEmptyList">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_margin">1dp</item>
        <item name="android:background">@color/x_ffffff</item>
        <item name="android:fontFamily">@font/regular_genesissansheadglobal</item>
        <item name="android:gravity">center</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">16dp</item>
        <item name="android:visibility">gone</item>
    </style>

    <!--내역이 없습니다2.-->
    <style name="ViewEmptyList2">
        <item name="android:layout_width">0dp</item>
