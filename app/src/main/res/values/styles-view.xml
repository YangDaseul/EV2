<resources>

    <!--황금색(cd9a81))테두리뷰-->
    <style name="ViewBackgroundTransStrokeToupe">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:fontFamily">@font/regular_genesissansheadglobal</item>
        <item name="android:textColor">@color/x_cd9a81</item>
        <item name="android:textSize">12sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:background">@drawable/bg_00000000_stroke_cd9a81</item>
        <item name="android:gravity">center</item>
    </style>

    <!--내역이 없습니다.-->
    <style name="ViewEmptyList">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_margin">1dp</item>
        <item name="android:background">@color/x_ffffff</item>
        <item name="android:fontFamily">@font/regular_genesissansheadglobal</item>
        <item name="android:gravity">center</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">2sp</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">16sp</item>
        <item name="android:visibility">gone</item>
    </style>

    <!-- 하단 승인 버튼(검은색)-->
    <style name="ViewBottomOkBtn">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/ripple_bg_111111</item>
        <item name="android:textColor">@color/x_ffffff</item>
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:gravity">center</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
    </style>

    <!-- ok 버튼(검은색)-->
    <style name="ViewOkBlackBtn">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/ripple_bg_141414</item>
        <item name="android:textColor">@color/x_ffffff</item>
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:gravity">center</item>
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
    </style>


    <!-- cancel 버튼(흰색)-->
    <style name="ViewCancelBtn">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/ripple_bg_ffffff_stroke_6f6f6f</item>
        <item name="android:textColor">@color/x_141414</item>
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:gravity">center</item>
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
    </style>


    <!-- 타이틀 바 오른쪽 텍스트 버튼 (연한 글씨) -->
    <style name="TitleBarTextBtn" parent="TextBtnBasic">
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:background">@drawable/bg_00000000_underline_000000</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:layout_marginEnd">20dp</item>
    </style>


    <!-- 입력레이아웃의 안내 메시지 타이틀 ex)xxx를 입력해주세요.-->
    <style name="ViewMsgInputTile">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">8sp</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">22sp</item>
    </style>


    <!-- 입력레이아웃의 서브 안내 메시지-->
    <style name="ViewMsgInputSub">
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">@color/x_aaabaf</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- 입력레이아웃의 서브 안내 메시지-->
    <style name="ViewMsgInputSub2">
        <item name="android:fontFamily">@font/regular_genesissansheadglobal</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">8sp</item>
        <item name="android:textColor">@color/x_262626</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">left</item>
    </style>

    <!-- aaabaf 사각형 마크-->
    <style name="ViewMarkGrey">
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:drawableLeft">@drawable/mark_aaabaf</item>
        <item name="android:drawablePadding">6dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- 000000 사각형 마크-->
    <style name="ViewMarkBlack">
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:drawableLeft">@drawable/mark_000000</item>
        <item name="android:drawablePadding">6dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- 입력레이아웃의 서브 타이틀-->
    <style name="ViewMsgInputSubTitle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/regular_genesissansheadglobal</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:textColor">@color/x_525252</item>
        <item name="android:textSize">14sp</item>
    </style>


    <!-- 입력레이아웃의 서브 메시지-->
    <style name="ViewMsgInputSubMsg">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">7sp</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">15sp</item>
        <item name="android:background">@color/x_f4f4f4</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingBottom">20dp</item>
    </style>


    <!-- 결과 버튼 큰 거 -->
    <style name="BigBtn" parent="TextBtnBasic">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:padding">18dp</item>
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="android:textSize">16sp</item>
    </style>

    <!--  바닥 하얀버튼 큰 거 -->
    <style name="BigBtn.White" parent="BigBtn">
        <item name="android:textColor">@color/x_141414</item>
        <item name="android:background">@drawable/ripple_bg_ffffff_stroke_6f6f6f</item>
    </style>

    <!--  바닥 검은버튼 큰 거 -->
    <style name="BigBtn.Black" parent="BigBtn">
        <item name="android:textColor">@color/x_ffffff</item>
        <item name="android:background">@drawable/ripple_bg_141414</item>
    </style>

    <!-- 서비스 이용불가 msg -->
    <style name="ServiceReq.Error" parent="TextBasic">
        <item name="android:fontFamily">@font/regular_genesissansheadglobal</item>
        <item name="android:textColor">@color/x_ba544d</item>
        <item name="android:textSize">14sp</item>
    </style>

    <!-- 서비스 신청 light 폰트  -->
    <style name="ServiceReq.Light" parent="TextBasic">
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="android:textColor">@color/x_525252</item>
    </style>

    <!-- 서비스 신청 대화상자 지금당장/예약 선택지 라디오버튼 텍스트 -->
    <style name="ServiceReqDialog" parent="TextBasic">
        <item name="android:layout_width">match_parent</item>
        <item name="android:button">@null</item>
        <item name="android:drawableStart">@drawable/radio_btn_selector</item>
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">15sp</item>
        <item name="android:drawablePadding">8dp</item>
    </style>

    <!-- 칸막이 회색 가로줄-->
    <style name="DummyGrayLineHorizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:layout_marginTop">40dp</item>
        <item name="android:src">@color/x_f4f4f4</item>
    </style>

    <style name="AmountTitleGold">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">16sp</item>
        <item name="android:textColor">@color/x_996449</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="AmountTitleBlack">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/medium_genesissanstextglobal</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">3sp</item>
        <item name="android:textColor">@color/x_111111</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="AmountMsg">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">3sp</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">12sp</item>
    </style>

</resources>
