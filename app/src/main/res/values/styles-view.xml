        <item name="android:textColor">@color/x_000000</item>
        <item name="android:background">@drawable/ripple_bg_ffffff_stroke_111111</item>
    </style>

    <!--  바닥 검은버튼 큰 거 -->
    <style name="BigBtn.Black" parent="BigBtn">
        <item name="android:textColor">@color/x_ffffff</item>
        <item name="android:background">@drawable/ripple_bg_111111</item>
    </style>

