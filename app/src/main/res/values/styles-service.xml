<resources>

    <!-- 서비스 메인 탭 텍스트 (정비/세차/대리) -->
    <style name="ServiceMainTabs" parent="TextBasic">
        <item name="android:fontFamily">@font/regular_genesissansheadglobal</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">@color/selector_color_000000_b9b9b9</item>
    </style>

    <!-- 대리운전 항목 설명-->
    <style name="ServiceDriveDesc" parent="TextBasic">
        <item name="android:textColor">@color/x_aaabaf</item>
        <item name="android:textSize">14dp</item>
        <item name="android:lineSpacingExtra">6dp</item>
    </style>

    <style name="ServiceDriveDesc.light" parent="ServiceDriveDesc">
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
    </style>

    <style name="ServiceDriveDesc.regular" parent="ServiceDriveDesc">
        <item name="android:fontFamily">@font/regular_genesissansheadglobal</item>
    </style>

    <!-- sonax 지점 검색 결과 없음   -->
    <style name="SonaxResult.NoResult" parent="TextBasic">
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/x_141414</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <!-- 세차 예약 내역 : -->
    <style name="CarWashHistoryLight" parent="TextBasic">
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="android:textColor">@color/x_000000</item>
    </style>
    <!-- 세차 예약 내역 : -->
    <style name="CarWashHistoryRegular" parent="TextBasic">
        <item name="android:fontFamily">@font/regular_genesissansheadglobal</item>
        <item name="android:textColor">@color/x_000000</item>
    </style>

    <!-- 세차 예약 내역 : -->
    <style name="CarWashHistoryBtn" parent="TextBtnBasic">
        <item name="android:layout_width">0dp</item>
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="android:textColor">@color/x_141414</item>
        <item name="android:textSize">16dp</item>
        <item name="android:background">@drawable/ripple_bg_ffffff_stroke_6f6f6f</item>
        <item name="android:padding">18dp</item>
    </style>

    <!-- 차종코드 -->
    <style name="ServiceDrive.RegularHead" parent="TextBasic">
        <item name="android:fontFamily">@font/regular_genesissansheadglobal</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">20dp</item>
    </style>

    <!-- 대리운전 light 폰트  -->
    <style name="ServiceDrive.Light" parent="TextBasic">
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="android:textColor">@color/x_525252</item>
    </style>


    <!-- 대리운전 신청 페이지/신청 결과 페이지 : 신청 내역 버튼 -->
    <style name="ServiceDrive.History" parent="TextBtnBasic">
        <item name="android:text">@string/service_drive_01</item>
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/x_6f6f6f</item>
        <item name="android:background">@drawable/ripple_bg_ffffff</item>
        <item name="android:layout_marginEnd">20dp</item>
    </style>

    <!-- 대리운전 신청 결과 출발/도착지 -->
    <style name="ServiceDriveReqResult.FromTo" parent="TextBasic">
        <item name="android:layout_width">0dp</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">15dp</item>
        <item name="android:paddingEnd">20dp</item>
        <item name="maxLines">2</item>
    </style>

    <!-- 대리운전 신청 결과 출발/도착지 이름  -->
    <style name="ServiceDriveReqResult.FromTo.Name" parent="ServiceDriveReqResult.FromTo">
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
    </style>

    <!-- 대리운전 신청 결과 출발/도착지 주소  -->
    <style name="ServiceDriveReqResult.FromTo.Address" parent="ServiceDriveReqResult.FromTo">
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
    </style>

    <!-- 대리운전 신청 결과 안내 -->
    <style name="ServiceDriveReqResult.Info" parent="TextBasic">
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
    </style>

    <!-- 대리운전 신청 상태 배경 -->
    <style name="ServiceDriveReqResultStatusBackground">
        <item name="android:background">@color/x_f4f4f4</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingBottom">20dp</item>
        <item name="android:paddingStart">15dp</item>
        <item name="android:paddingEnd">15dp</item>
    </style>

    <!-- 대리운전 신청 상태 텍스트 -->
    <style name="ServiceDriveReqResult.Status.Text" parent="TextBasic">
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">15dp</item>
    </style>

    <style name="ServiceDriveReqResult.Status.Text.Light" parent="ServiceDriveReqResult.Status.Text">
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
    </style>

    <style name="ServiceDriveReqResult.Status.Text.Regular" parent="ServiceDriveReqResult.Status.Text">
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
    </style>


    <!-- 대리운전 신청내역 -->
    <!-- 패널, 데이터 이름 -->
    <style name="ServiceDrive.History.Data.Title" parent="TextBasic">
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">13dp</item>
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="layout_constraintVertical_chainStyle">packed</item>
    </style>

    <!-- 패널, 데이터 값 -->
    <style name="ServiceDrive.History.Data.Desc" parent="TextBasic">
        <item name="android:layout_width">0dp</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">16dp</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:layout_marginEnd">10dp</item>
    </style>

    <!-- 신청내역 펼쳤을 때 안내문구 세 줄 -->
    <style name="ServiceDrive.History.Noti" parent="TextBasic">
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_marginStart">15dp</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:textColor">@color/x_aaabaf</item>
        <item name="android:textSize">14dp</item>
    </style>


    <!-- 하자 재발 신청 내역 아이템-->
    <!-- 왼쪽 동그란 텍스트 -->
    <style name="RelapseHistoryHeader" parent="TextBasic">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- 항목 이름-->
    <style name="RelapseHistoryItemTitle" parent="TextBasic">
        <item name="android:textColor">@color/x_525252</item>
        <item name="android:textSize">16dp</item>
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="layout_constraintVertical_chainStyle">packed</item>
    </style>

    <!-- 항목 항목 값-->
    <style name="RelapseHistoryItemDesc" parent="TextBasic">
        <item name="android:layout_width">0dp</item>
        <item name="android:textColor">@color/x_141414</item>
        <item name="android:textSize">16dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
    </style>

    <!-- 하자 재발 내역 목록 밑에 안내문 세 개 -->
    <style name="RelapseHistoryItemNoti" parent="TextBasic">
        <item name="android:textColor">@color/x_aaabaf</item>
        <item name="android:textSize">14dp</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:lineSpacingExtra">6dp</item>
    </style>

    <!-- 하자 재발 3단계 4회이상 테두리 -->
    <style name="Relapse3RepairOver4Selected" parent="TextBasic">
        <item name="android:layout_width">0dp</item>
        <item name="android:background">@drawable/bg_ffffff_stroke_141414_2dp</item>
        <item name="android:padding">18dp</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintHorizontal_chainStyle">packed</item>
    </style>

    <!-- 하자 재발 3단계 4회이상 버튼 -->
    <style name="Relapse3RepairOver4Btn" parent="TextBtnBasic">
        <item name="android:layout_width">0dp</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:background">@drawable/ripple_bg_ffffff</item>
        <item name="android:padding">16dp</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintHorizontal_chainStyle">packed</item>
    </style>


    <!-- 하자 재발 3단계 과거 수리내역 레이아웃 -->
    <style name="Relapse3RepairHistoryTextInputLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="boxBackgroundColor">@color/x_ffffff</item>
        <item name="boxBackgroundMode">outline</item>
        <item name="boxCornerRadiusTopEnd">0dp</item>
        <item name="boxCornerRadiusTopStart">0dp</item>
        <item name="boxStrokeColor">@color/x_141414</item>
        <item name="boxStrokeErrorColor">@color/x_ba544d</item>
        <item name="endIconMode">clear_text</item>
        <item name="errorEnabled">true</item>
        <item name="errorIconTint">@color/x_ba544d</item>
        <item name="errorTextColor">@color/x_ba544d</item>
        <item name="hintEnabled">true</item>
        <item name="hintTextColor">@color/x_000000</item>
        <item name="hintTextAppearance">@style/CommonTextInputEditTextHint</item>
        <item name="placeholderTextAppearance">@style/CommonTextInputEditTextHint</item>
        <item name="errorTextAppearance">@style/CommonTextInputEditTextError</item>
    </style>

    <!-- 과거수리내역 입력창 -->
    <style name="Relapse3RepairHistoryTextInputEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:inputType">text</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:padding">16dp</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textCursorDrawable">@drawable/cursor_141414</item>
        <item name="android:textSize">15dp</item>
    </style>

    <!-- 하자재발3 수리내역 날짜선택 타이틀-->
    <style name="Relapse3RepairHistoryDatePickTitle" parent="TextBasic">
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">13dp</item>
    </style>

    <!-- 하자재발3 수리내역 날짜선택 오류 메시지 -->
    <style name="Relapse3RepairHistoryDateError" parent="TextBasic">
        <item name="android:text">@string/relapse_3_repair_date_error</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:textColor">@color/x_ba544d</item>
        <item name="android:textSize">12dp</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:visibility">gone</item>
    </style>

    <!-- 하자재발3 수리내역 날짜선택 버튼-->
    <style name="Relapse3RepairHistoryDatePickBtn" parent="TextBtnBasic">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:background">@drawable/ripple_bg_ffffff_stroke_141414</item>
        <item name="android:drawableRight">@drawable/ic_calendar</item>
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">16dp</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">15dp</item>
    </style>


    <!-- 하자재발 현황 조회 상단 "접수일" "상태" -->
    <style name="RelapseReqResultTopTitle" parent="TextBasic">
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
        <item name="android:textSize">14dp</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <!-- 하자재발 현황조회 구간별 제목  -->
    <style name="RelapseReqResultSubTitle" parent="TextBasic">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginTop">40dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:background">@drawable/bg_00000000_underline_000000</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:fontFamily">@font/regular_genesissansheadglobal</item>
        <item name="android:textColor">@color/x_000000</item>
        <item name="android:textSize">18dp</item>
    </style>

    <!-- 하자재발 현황조회 항목 공통  -->
    <style name="RelapseReqResultItem" parent="TextBasic">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:textColor">@color/x_000000</item>
    </style>

    <!-- 하자재발 현황조회 항목 제목  -->
    <style name="RelapseReqResultItem.Title" parent="RelapseReqResultItem">
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:textSize">13dp</item>
        <item name="android:fontFamily">@font/light_genesissansheadglobal</item>
    </style>

    <!-- 하자재발 현황조회 항목 값 -->
    <style name="RelapseReqResultItem.Desc" parent="RelapseReqResultItem">
        <item name="android:fontFamily">@font/regular_genesissanstextglobal</item>
        <item name="android:textSize">15dp</item>

    </style>

</resources>
